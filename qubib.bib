@InProceedings{CGHS18post,
  author    = {Czajkowski, Jan and Groot Bruinderink, Leon and H{\"u}lsing, Andreas and Schaffner, Christian and Unruh, Dominique},
  booktitle = {Post-Quantum Cryptography},
  title     = {Post-quantum Security of the Sponge Construction},
  year      = {2018},
  address   = {Cham},
  editor    = {Lange, Tanja and Steinwandt, Rainer},
  pages     = {185--204},
  publisher = {Springer International Publishing},
  abstract  = {We investigate the post-quantum security of hash
                  functions based on the sponge construction. A
                  crucial property for hash functions in the
                  post-quantum setting is the collapsing property (a
                  strengthening of collision-resistance). We show that
                  the sponge construction is collapsing (and in
                  consequence quantum collision-resistant) under
                  suitable assumptions about the underlying block
                  function. In particular, if the block function is a
                  random function or a (non-invertible) random
                  permutation, the sponge construction is
                  collapsing. We also give a quantum algorithm for
                  finding collisions in an arbitrary function. For the
                  sponge construction, the algorithm complexity
                  asymptotically matches the complexity implied by
                  collision resistance.},
  isbn      = {978-3-319-79063-3},
  oldkey    = {#CGHS+18#},
