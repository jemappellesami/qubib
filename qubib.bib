@Article{MNRS06search,
  author        = {Magniez, Frédéric and Nayak, Ashwin and Roland, Jérémie and Santha, Miklos},
  journal       = {SIAM Journal on Computing, 40(1):142-164, 2011},
  title         = {Search via Quantum Walk},
