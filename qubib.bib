@Article{MPBM16experimental,
  author   = {McCutcheon, W. and Pappa, A. and Bell, B. A. and McMillan, A. and Chailloux, A. and Lawson, T. and Mafu, M. and Markham, D. and Diamanti, E. and Kerenidis, I. and Rarity, J. G. and Tame, M. S.},
  journal  = {Nature Communications},
  title    = {Experimental verification of multipartite entanglement in quantum networks},
  year     = {2016},
  issn     = {2041-1723},
  month    = {Nov},
  number   = {1},
  pages    = {13251},
  volume   = {7},
  abstract = {Multipartite entangled states are a fundamental
                  resource for a wide range of quantum information
                  processing tasks. In particular, in quantum
                  networks, it is essential for the parties involved
                  to be able to verify if entanglement is present
                  before they carry out a given distributed task. Here
                  we design and experimentally demonstrate a protocol
                  that allows any party in a network to check if a
                  source is distributing a genuinely multipartite
                  entangled state, even in the presence of untrusted
                  parties. The protocol remains secure against
                  dishonest behaviour of the source and other parties,
                  including the use of system imperfections to their
                  advantage. We demonstrate the verification protocol
                  in a three- and four-party setting using
                  polarization-entangled photons, highlighting its
                  potential for realistic photonic quantum
                  communication and networking applications.},
  day      = {09},
  doi      = {10.1038/ncomms13251},
  source   = {Luka Music, 2021-09-16},
  url      = {https://doi.org/10.1038/ncomms13251},
}

