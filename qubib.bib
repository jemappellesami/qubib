@InProceedings{ABBC13sphf,
  author    = {Abdalla, Michel and Benhamouda, Fabrice and Blazy, Olivier and Chevalier, C{\'e}line and Pointcheval, David},
  booktitle = {Advances in Cryptology - ASIACRYPT 2013},
