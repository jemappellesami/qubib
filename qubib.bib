@Article{XTH20improved,
  author         = {Xu, Qingshan and Tan, Xiaoqing and Huang, Rui},
  journal        = {Entropy},
  title          = {Improved Resource State for Verifiable Blind Quantum Computation},
  year           = {2020},
  issn           = {1099-4300},
  number         = {9},
  volume         = {22},
  abstract       = {Recent advances in theoretical and experimental
                  quantum computing raise the problem of verifying the
                  outcome of these quantum computations. The recent
                  verification protocols using blind quantum computing
                  are fruitful for addressing this
                  problem. Unfortunately, all known schemes have
                  relatively high overhead. Here we present a novel
                  construction for the resource state of verifiable
                  blind quantum computation. This approach achieves a
                  better verifiability of 0.866 in the case of
                  classical output. In addition, the number of
                  required qubits is 2N+4cN, where N and c are the
                  number of vertices and the maximal degree in the
                  original computation graph, respectively. In other
                  words, our overhead is less linear in the size of
                  the computational scale. Finally, we utilize the
                  method of repetition and fault-tolerant code to
                  optimise the verifiability.},
  article-number = {996},
  doi            = {10.3390/e22090996},
  source         = {Luka Music, 2021-09-16},
  url            = {https://www.mdpi.com/1099-4300/22/9/996},
}

